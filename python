def positive(constant, variable):
    sum = 0
    list_const = list(constant)
    list_var = list(variable)
    for i in range(len(list_const)):
        if list_const[i] == list_var[i]:
            sum += 1
    return sum


def check(list_constant, i):
    add = 0
    for a in range(i):
        if list_constant[i] == list_constant[a]:
            add += 1
    if add == 0:
        return True
    else:
        return False

def negative(constant, variable):
    list_constant = list(constant)
    list_var = list(variable)
    sum = 0
    for i in range(len(list_constant)):
        if list_constant[i] == list_var[i]:
            list_constant[i] = "*"
            list_var[i] = "/"


    for i in range(len(list_constant)):
        if check(list_constant,i):

            for o in range(len(list_var)):
                if list_constant[i] == list_var[o]:
                    sum += 1
                    break
    return sum


def nnegative(constant, variable):
    sum = 0
    list_constant = list(constant)
    list_var = list(variable)
    if positive(constant, variable) == 0:
