                for o in range(len(variable)):
                    if constant[i] == variable[o] and list_constant[i] != list_var[i]:
                        sum += 1
                        break
    return sum


n = int(input(""))
secret = int(input(""))
secret_code = str(secret)

guess = ""
if n == 4:
    guess = "1000"
    guess_list = ["1000"]
if n == 5:
    guess = "10000"
    guess_list = ["10000"]
if n == 6:
    guess = "100000"
    guess_list = ["100000"]
positive_list = []
negative_list = []

positive_list.append(positive(secret_code, guess))
negative_list.append(negative(secret_code, guess))


def check_positive(guess_list, positive_list, guess):
    sum = 0
    for c in range(len(guess_list)):
        if positive(guess, guess_list[c]) == positive_list[c]:
            sum += 1
    if sum == len(guess_list):
        return True
    else:
        return False


def check_negative(guess_list, negative_list, guess):
    sum = 0
    for c in range(len(guess_list)):
        if negative(guess, guess_list[c]) == negative_list[c]:
            sum += 1
    if sum == len(guess_list):
        return True
    else:
        return False


for u in range(secret):
    int_guess = int(guess)
    int_guess += 1
    guess = str(int_guess)

    if check_positive(guess_list, positive_list, guess) and check_negative(guess_list, negative_list, guess):
        guess_list.append(guess)
        positive_list.append(positive(secret_code, guess))
        negative_list.append(negative(secret_code, guess))

    if int_guess == secret:
        break
liste = []
for i in range(len(guess_list)):
    a = int(guess_list[i])
    liste.append(a)

for i in range(len(guess_list)):
    print(guess_list[i])
print("Found the secret code", secret)
print("Guess list: ", liste)
print("# of digits in correct position:   ", positive_list)
print("# of digits in incorrect position: ", negative_list)
